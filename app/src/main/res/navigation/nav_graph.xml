    </fragment>

    <fragment
        android:id="@+id/ThirdFragment"
        android:name="de.tubs.cs.ias.fbadstestbed.ThirdFragment"
        android:label="Init"
        tools:layout="@layout/third_state">

        <action
            android:id="@+id/action_ThirdFragment_to_FourthFragment"
            app:destination="@id/FourthFragment" />
    </fragment>

    <fragment
        android:id="@+id/FourthFragment"
        android:name="de.tubs.cs.ias.fbadstestbed.FourthFragment"
        android:label="Consent"
        tools:layout="@layout/fourth_state">

        <action
            android:id="@+id/action_FourthFragment_to_FifthFragment"
